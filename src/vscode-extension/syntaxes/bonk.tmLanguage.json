{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Bonk",
  "patterns": [
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "strings": {
      "name": "string.quoted.double.bonk",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.bonk",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.bonk"
}
