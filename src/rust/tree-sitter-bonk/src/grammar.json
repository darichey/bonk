{
  "name": "bonk",
  "rules": {
    "ledger": {
      "type": "FIELD",
      "name": "transaction",
      "content": {
        "type": "REPEAT",
        "content": {
          "type": "SYMBOL",
          "name": "transaction"
        }
      }
    },
    "transaction": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "date",
          "content": {
            "type": "SYMBOL",
            "name": "date"
          }
        },
        {
          "type": "FIELD",
          "name": "description",
          "content": {
            "type": "SYMBOL",
            "name": "description"
          }
        },
        {
          "type": "FIELD",
          "name": "posting",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "posting"
            }
          }
        }
      ]
    },
    "posting": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "account",
          "content": {
            "type": "SYMBOL",
            "name": "account"
          }
        },
        {
          "type": "FIELD",
          "name": "amount",
          "content": {
            "type": "SYMBOL",
            "name": "amount"
          }
        }
      ]
    },
    "date": {
      "type": "PATTERN",
      "value": "\\d{4}-\\d{2}-\\d{2}"
    },
    "description": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\[\"\\\\bnfrt])*\""
    },
    "account": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*(:[A-Za-z_][A-Za-z0-9_]*)*"
    },
    "amount": {
      "type": "PATTERN",
      "value": "-?\\d+(\\.\\d+)?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

