{
  "name": "bonk",
  "rules": {
    "ledger": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "transaction"
      }
    },
    "transaction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "posting"
          }
        }
      ]
    },
    "posting": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "account"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "amount"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "date": {
      "type": "PATTERN",
      "value": "\\d{4}-\\d{2}-\\d{2}"
    },
    "description": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\[\"\\\\bnfrt])*\""
    },
    "account": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*(:[A-Za-z_][A-Za-z0-9_]*)*"
    },
    "amount": {
      "type": "PATTERN",
      "value": "-?\\d+(\\.\\d+)?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

